{
  "name": "Quick Folders",
  "identifier": "com.jakecalkins.quickfolders",
  "version": "2.1.0",
  "description": "Adds a Quick Folders window to browse and open local folders quickly. Navigate your media library with customizable filters, keyboard shortcuts, and deep folder indexing.",
  "author": {
    "name": "JakeCalkins",
    "url": "https://github.com/JakeCalkins/iina-quick-folders-plugin"
  },
  "ghRepo": "JakeCalkins/iina-quick-folders-plugin",
  "ghVersion": 3,
  "entry": "main.js",
  "permissions": [
    "file-system",
    "show-osd"
  ],
  "allowedDomains": [
    "*"
  ],
  "preferencesPage": "preferences.html",
  "preferenceDefaults": {
    "filterImages": false,
    "filterAudio": false,
    "videoOnly": false,
    "maxIndexDepth": 3,
    "openWindowShortcut": "cmd+shift+a",
    "addFolderShortcut": "n"
  }
}